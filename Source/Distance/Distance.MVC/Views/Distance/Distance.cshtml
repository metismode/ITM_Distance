        var myLatlng = new google.maps.LatLng(14.466991, 100.545021);
        var mapOptions = {
            zoom: 7,
            center: myLatlng
        };
        map = new google.maps.Map(document.getElementById('map-canvas'), mapOptions);
        directionsDisplay.setMap(map);


        var start = myLatlngO;
        var end = myLatlngD;
        var request = {
            origin: start,
            destination: end,
